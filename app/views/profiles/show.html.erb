        <h1 class="page-header">
            Perfil <small><%= @profile.long_name %> de <%= @organization.name %></small>
        </h1>
        <ol class="breadcrumb">
            <li class="active">
                <i class="fa fa-dashboard"></i> <%= link_to 'Organizaciones', organization_path(@organization) %> > <%= link_to 'Perfiles', organization_profiles_path(@organization) %> > Editar
            </li>
        </ol>
    </div>
</div>
