  <div class="col-md-6">
  <p class="form-group">
    <strong>Long name:</strong> 
    <%= @profile.long_name %>
  </p>
  <p class="form-group">
    <strong>Name:</strong> 
    <%= @profile.name %>
  </p>
  </div>
  <div class="col-md-6"></div>
</div>

